{
    "version": 2,
    "builds": [
        {
            "src": "/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/robots.txt",
            "dest": "/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}